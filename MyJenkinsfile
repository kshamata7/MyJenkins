pipeline {
    agent any
    stages {
        stage('World') {
            steps {
                echo 'Hello World from MyJenkins'
            }
        }
    stage('India') {
            steps {
                echo 'Hello India'
            }
        }   
    stage('Telangana') {
            steps {
                echo 'Hello Telangana'
            }
        }
    }
}
